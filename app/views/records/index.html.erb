<div class="col-md-12">
  <h3><%= t(:record_list) %></h3>
</div>
<div class="col-md-12">
  <table class="table">
    <thead>
      <tr>
        <th><%= Record.human_attribute_name(:id) %></th>
        <th><%= Record.human_attribute_name(:user_id) %></th>
        <th><%= Record.human_attribute_name(:superior) %></th>
        <th><%= Record.human_attribute_name(:product_id) %></th>
        <th><%= Record.human_attribute_name(:sell_date) %></th>
        <th><%= Record.human_attribute_name(:customer_name) %></th>
        <th><%= Record.human_attribute_name(:customer_cell) %></th>
        <th><%= Record.human_attribute_name(:qty) %></th>
        <th><%= Record.human_attribute_name(:total_prize) %></th>
        <th><%= Record.human_attribute_name(:status) %></th>
        <th colspan="3"><%= t(:operations) %></th>
      </tr>
    </thead>
    <tbody>
      <%= render @records %>
    </tbody>
  </table>
</div>
